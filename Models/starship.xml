<?xml version="1.0"?>

<PropertyList>

  <path>starship.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>fuselage</object-name>
    <object-name>interieur</object-name>
    <object-name>separation</object-name>
    <object-name>planchet</object-name>
    <object-name>porte</object-name>
    <object-name>ailes</object-name>
    <object-name>aileronG</object-name>
    <object-name>voletGE</object-name>
    <object-name>voletGI</object-name>
    <object-name>aileronD</object-name>
    <object-name>voletDE</object-name>
    <object-name>voletDI</object-name>
    <object-name>canardG</object-name>
    <object-name>canardD</object-name>
    <object-name>profondeurG</object-name>
    <object-name>profondeurD</object-name>
    <object-name>derives</object-name>
    <object-name>directionG</object-name>
    <object-name>directionD</object-name>
    <object-name>axeA</object-name>
    <object-name>axeB</object-name>
    <object-name>roueA</object-name>
    <object-name>porteAG1</object-name>
    <object-name>porteAG2</object-name>
    <object-name>porteAD1</object-name>
    <object-name>porteAD2</object-name>
    <object-name>trainG1</object-name>
    <object-name>trainG2</object-name>
    <object-name>roueG</object-name>
    <object-name>porteG1</object-name>
    <object-name>porteG2</object-name>
    <object-name>porteG3</object-name>
    <object-name>trainD1</object-name>
    <object-name>trainD2</object-name>
    <object-name>roueD</object-name>
    <object-name>porteD1</object-name>
    <object-name>porteD2</object-name>
    <object-name>porteD3</object-name>
    <object-name>bolG</object-name>
    <object-name>bolD</object-name>
    <object-name>heliceG</object-name>
    <object-name>heliceD</object-name>

    <!-- Objets transparents -->
    <object-name>vitres</object-name>
    <object-name>propdiscG</object-name>
    <object-name>propblurG</object-name>
    <object-name>propdiscD</object-name>
    <object-name>propblurD</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitres</object-name>
    <object-name>propdiscG</object-name>
    <object-name>propblurG</object-name>
    <object-name>propdiscD</object-name>
    <object-name>propblurD</object-name>
  </animation>

  <!-- Pilote -->
  <model>
    <name>Pilote</name>
    <path>Aircraft/Starship/Models/Pilot/pilot.xml</path>
    <offsets>
      <x-m> -3.572 </x-m>
      <y-m> -0.407 </y-m>
      <z-m> -0.027 </z-m>
    </offsets>
  </model>

  <!-- Pilote invisible depuis le cockpit -->
  <animation>
    <type>select</type>
    <object-name>Pilote</object-name>
    <condition>
      <greater-than>
        <property>sim/current-view/view-number</property>
        <value>0</value>
      </greater-than>
    </condition>
  </animation>

  <!-- Planches de bord -->
  <model>
    <path>Aircraft/Starship/Models/Panel/panel.xml</path>
    <offsets>
      <x-m> -4.287 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.051 </z-m>
    </offsets>
  </model>

  <!-- Rentré des roues Temporairement disparition/apparition des roues -->
  <animation>
    <type>select</type>
    <object-name>roueA</object-name>
    <condition>
      <not>
        <equals>
          <property>gear/gear[0]/position-norm</property>
          <value>0</value>
        </equals>
      </not>
    </condition>
  </animation>

  <!-- Géométrie variable des plans "canard" -->
  <animation>
    <type>rotate</type>
    <object-name>canardG</object-name>
    <object-name>profondeurG</object-name>
    <condition>
      <greater-than>
        <property>velocities/airspeed-kt</property>
        <value>150.00</value>
      </greater-than>
    </condition>
    <factor> 20.00 </factor>
    <center>
     <x-m> -6.039 </x-m>
     <y-m> -0.146 </y-m>
     <z-m> -0.313 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 0.000 </y>
      <z> 1.000 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>canardD</object-name>
    <object-name>profondeurD</object-name>
    <condition>
      <greater-than>
        <property>velocities/airspeed-kt</property>
        <value>150.00</value>
      </greater-than>
    </condition>
    <factor> 20.00 </factor>
    <center>
     <x-m> -6.039 </x-m>
     <y-m>  0.146 </y-m>
     <z-m> -0.313 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y>  0.000 </y>
      <z> -1.000 </z>
    </axis>
  </animation>

  <!-- L'hélice gauche--> 
  <!-- Séléction de l'objet visible en fonction des rpm -->
  <animation>
    <type>select</type>
    <object-name>propdiscG</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>1500</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblurG</object-name>
    <condition>
      <and>
        <greater-than>
          <property>engines/engine[0]/rpm</property>
          <value>600.000</value>
        </greater-than>
        <less-than>
          <property>engines/engine[0]/rpm</property>
          <value>1500.001</value>
        </less-than>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>heliceG</object-name>
    <condition>
      <less-than>
        <property>engines/engine[0]/rpm</property>
	<value>1500</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>heliceG</object-name>
    <object-name>bolG</object-name>
    <object-name>propdiscG</object-name>
    <object-name>propblurG</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor> 1 </factor>
    <center>
     <x-m>  5.568 </x-m>
     <y-m> -1.558 </y-m>
     <z-m>  0.191 </z-m>
    </center>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

<!-- L'hélice droite--> 
<!-- Séléction de l'objet visible en fonction des rpm -->
  <animation>
    <type>select</type>
    <object-name>propdiscD</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[1]/rpm</property>
        <value>1500</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblurD</object-name>
    <condition>
      <and>
        <greater-than>
          <property>engines/engine[1]/rpm</property>
          <value>600.000</value>
        </greater-than>
        <less-than>
          <property>engines/engine[1]/rpm</property>
          <value>1500.001</value>
        </less-than>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>heliceD</object-name>
    <condition>
      <less-than>
        <property>engines/engine[1]/rpm</property>
	<value>1500</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>heliceD</object-name>
    <object-name>bolD</object-name>
    <object-name>propdiscD</object-name>
    <object-name>propblurD</object-name>
    <property>engines/engine[1]/rpm</property>
    <factor> 1 </factor>
    <center>
     <x-m> 5.568 </x-m>
     <y-m> 1.558 </y-m>
     <z-m> 0.191 </z-m>
    </center>
    <axis>
      <x> -1.000 </x>
      <y>  0.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <!-- Gouverne de profondeur -->
  <animation>
    <type>rotate</type>
    <object-name>profondeurG</object-name>
    <property>/controls/flight/elevator</property>
    <factor>-11</factor>
    <axis>
      <x1-m> -5.151 </x1-m>
      <y1-m> -3.658 </y1-m>
      <z1-m> -0.313 </z1-m>
      <x2-m> -5.610 </x2-m>
      <y2-m> -0.591 </y2-m>
      <z2-m> -0.313 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>profondeurD</object-name>
    <property>/controls/flight/elevator</property>
    <factor>-11</factor>
    <axis>
      <x1-m> -5.610 </x1-m>
      <y1-m>  0.591 </y1-m>
      <z1-m> -0.313 </z1-m>
      <x2-m> -5.151 </x2-m>
      <y2-m>  3.658 </y2-m>
      <z2-m> -0.313 </z2-m>
    </axis>
  </animation>

  <!-- Gouverne de direction -->
  <animation>
    <type>rotate</type>
    <object-name>directionG</object-name>
    <property>/controls/flight/rudder</property>
    <factor>20</factor>
    <axis>
      <x1-m>  6.202 </x1-m>
      <y1-m> -8.276 </y1-m>
      <z1-m> -0.365 </z1-m>
      <x2-m>  6.848 </x2-m>
      <y2-m> -8.054 </y2-m>
      <z2-m>  1.590 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>directionD</object-name>
    <property>/controls/flight/rudder</property>
    <factor>20</factor>
    <axis>
      <x1-m>  6.202 </x1-m>
      <y1-m>  8.276 </y1-m>
      <z1-m> -0.365 </z1-m>
      <x2-m>  6.848 </x2-m>
      <y2-m>  8.054 </y2-m>
      <z2-m>  1.590 </z2-m>
    </axis>
  </animation>

  <!-- Animations des ailerons -->
  <animation>
    <type>rotate</type>
    <object-name>aileronG</object-name>
    <property>/surface-positions/left-aileron-pos-norm</property>
