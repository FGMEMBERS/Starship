    <axis>
      <x1-m>  5.857 </x1-m>
      <y1-m> -8.185 </y1-m>
      <z1-m> -0.319 </z1-m>
      <x2-m>  5.007 </x2-m>
      <y2-m> -5.125 </y2-m>
