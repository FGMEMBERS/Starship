      <x1-m>  5.007 </x1-m>
      <y1-m>  5.125 </y1-m>
      <z1-m> -0.368 </z1-m>
      <x2-m>  5.857 </x2-m>
      <y2-m>  8.185 </y2-m>
      <z2-m> -0.319 </z2-m>
    </axis>
  </animation>

  <!-- Volets -->
  <animation>
    <type>rotate</type>
    <object-name>voletGE</object-name>
    <property>surface-positions/flap-pos-norm</property>
