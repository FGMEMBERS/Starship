  <animation>
    <type>rotate</type>
    <object-name>voletDI</object-name>
    <property>surface-positions/flap-pos-norm</property>
    <factor>50.0</factor>
    <axis>
      <x1-m>  3.969 </x1-m>
      <y1-m>  2.226 </y1-m>
      <z1-m> -0.514 </z1-m>
      <x2-m>  4.387 </x2-m>
      <y2-m>  3.595 </y2-m>
      <z2-m> -0.489 </z2-m>
    </axis>
  </animation>

  <!-- Rentrée des roues -->
  <animation>
    <type>rotate</type>
    <object-name>axeA</object-name>
    <object-name>axeB</object-name>
    <object-name>roueA</object-name>
    <property>gear/gear[0]/position-norm</property>
    <interpolation>
      <entry>
        <ind>  0.0 </ind>
        <dep> 95.0 </dep>
      </entry>
      <entry>
        <ind>  0.3 </ind>
        <dep> 95.0 </dep>
      </entry>
      <entry>
        <ind> 1.0</ind>
        <dep> 0.0</dep>
      </entry>
    </interpolation>
    <center>
      <x-m> -5.112 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.584 </z-m>
    </center>
    <axis>
      <x>  0.00 </x>
      <y>  1.00 </y>
      <z>  0.00 </z>
    </axis>
  </animation>

  <animation>
