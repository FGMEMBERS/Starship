  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <condition>
      <property>sim/rendering/rembrandt/enabled</property>
    </condition>
    <object-name>vitre</object-name>
  </effect>

  <animation>
    <type>select</type>
    <object-name>FDbars</object-name>
    <condition>
      <equals>
        <property>/instrumentation/flightdirector/fd-on</property>
        <value>1</value>
      </equals>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>fd.annun</object-name>
    <condition>
      <equals>
        <property>/instrumentation/flightdirector/fd-on</property>
        <value>1</value>
      </equals>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>ap.annun</object-name>
    <condition>
      <equals>
        <property>/instrumentation/flightdirector/autopilot-on</property>
        <value>1</value>
      </equals>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>GSscale</object-name>
    <condition>
      <property>/instrumentation/nav/has-gs</property>
      <property>/instrumentation/nav/in-range</property>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>GSneedle</object-name>
    <condition>
      <property>/instrumentation/nav/has-gs</property>
      <property>/instrumentation/nav/in-range</property>
    </condition>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>GSneedle</object-name>
    <property>/instrumentation/nav/gs-needle-deflection</property>
    <interpolation>
      <entry><ind> -10 </ind><dep> -0.0308 </dep></entry>
      <entry><ind>   0 </ind><dep>  0.0000 </dep></entry>
      <entry><ind>  10 </ind><dep>  0.0308 </dep></entry>
    </interpolation>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Omarker</object-name>
    <condition>
      <equals>
        <property>/instrumentation/marker-beacon/outer</property>
        <value>true</value>
      </equals>
    </condition>
  </animation>
 
  <animation>
    <type>select</type>
    <object-name>Mmarker</object-name>
    <condition>
      <equals>
        <property>/instrumentation/marker-beacon/middle</property>
        <value>true</value>
      </equals>
    </condition>
  </animation>
 
  <animation>
    <type>select</type>
    <object-name>Imarker</object-name>
    <condition>
      <equals>
        <property>/instrumentation/marker-beacon/inner</property>
        <value>true</value>
      </equals>
    </condition>
  </animation>
 
  <animation>
    <type>select</type>
    <object-name>LOCscale</object-name>
    <condition>
      <property>/instrumentation/nav/nav-loc</property>
      <property>/instrumentation/nav/in-range</property>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>LOCneedle</object-name>
    <condition>
      <property>/instrumentation/nav/nav-loc</property>
      <property>/instrumentation/nav/in-range</property>
    </condition>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>LOCneedle</object-name>
    <property>/instrumentation/nav/heading-needle-deflection</property>
    <interpolation>
      <entry><ind> -10 </ind><dep> -0.0253 </dep></entry>
      <entry><ind>   0 </ind><dep>  0.0000 </dep></entry>
      <entry><ind>  10 </ind><dep>  0.0253 </dep></entry>
    </interpolation> 
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>pfdhdg</object-name>
    <property>autopilot/internal/fdm-heading-bug-error-deg</property>
    <interpolation>
      <entry><ind> -180 </ind><dep> -50 </dep></entry>
      <entry><ind>  -50 </ind><dep> -50 </dep></entry>
      <entry><ind>    0 </ind><dep>   0 </dep></entry>
      <entry><ind>   50 </ind><dep>  50 </dep></entry>
      <entry><ind>  180 </ind><dep>  50 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.007 </x-m>
      <y-m> -0.013 </y-m>
      <z-m> -0.135 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>FDbars</object-name>
    <property>/instrumentation/flightdirector/vbar-roll</property>
    <center>
      <x-m>  0.007 </x-m>
      <y-m> -0.014 </y-m>
      <z-m>  0.005 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>FDbars</object-name>
    <property>/instrumentation/flightdirector/vbar-pitch</property>
    <factor>0.0014</factor>
    <center>
      <x-m>  0.007 </x-m>
      <y-m> -0.014 </y-m>
      <z-m>  0.005 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>
 
  <animation>
    <type>texmultiple</type>
    <object-name>AI</object-name>
    <transform>
      <property>/orientation/pitch-deg</property>
      <subtype>textranslate</subtype>
      <factor>0.00314</factor>
      <axis>
        <x> 0 </x>
        <y> 1 </y>
        <z> 0 </z>
      </axis>
    </transform>
    <transform>
      <property>/orientation/roll-deg</property>
      <subtype>texrotate</subtype>
      <center>
        <x> 0.25 </x>
        <y> 0.50 </y>
      </center>
      <axis>
        <x>  0 </x>
        <y>  0 </y>
        <z> -1 </z>
      </axis>
    </transform>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>rollptr</object-name>
    <property>/orientation/roll-deg</property>
    <center>
      <x-m>  0.000 </x-m>
      <y-m> -0.014 </y-m>
      <z-m>  0.005 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
 
  <animation>
    <type>texrotate</type>
    <object-name>HDGface</object-name>
    <property>/orientation/heading-magnetic-deg</property>
    <center>
      <x> 0.75 </x>
      <y> 0.75 </y>
      <z> 0.00 </z>
    </center>
    <axis>
      <x>  0 </x>
      <y>  0 </y>
      <z> -1 </z>
    </axis>
  </animation>
  
  <animation>
    <type>textranslate</type>
    <object-name>ASIribbon</object-name>
    <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
    <interpolation>
      <entry><ind>   0 </ind><dep> 0.0000 </dep></entry>
      <entry><ind> 100 </ind><dep> 0.1490 </dep></entry>
      <entry><ind> 200 </ind><dep> 0.2980 </dep></entry>
      <entry><ind> 300 </ind><dep> 0.4455 </dep></entry>
      <entry><ind> 400 </ind><dep> 0.5945 </dep></entry>
      <entry><ind> 500 </ind><dep> 0.7425 </dep></entry>
      <entry><ind> 600 </ind><dep> 0.8890 </dep></entry>
    </interpolation>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
 
  <animation>
    <type>textranslate</type>
    <object-name>Asitext</object-name>
    <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
    <factor>0.001</factor>
    <step>100</step>
    <scroll>1</scroll>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>textranslate</type>
    <object-name>Asitext.001</object-name>
    <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
    <factor>0.01</factor>
    <step>10</step>
    <scroll>1</scroll>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>textranslate</type>
    <object-name>Asitext.002</object-name>
    <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
    <factor>0.1</factor>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>translate</type>
    <object-name>VSIneedle</object-name>
    <property>/autopilot/internal/vert-speed-fpm</property>
    <interpolation>
      <entry><ind> -6000 </ind><dep> -0.037 </dep></entry>
      <entry><ind>     0 </ind><dep>  0.000 </dep></entry>
      <entry><ind>  6000 </ind><dep>  0.037 </dep></entry>
    </interpolation>
    <center>
      <x-m> 0.000 </x-m>
      <y-m> 0.084 </y-m>
      <z-m> 0.004 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>VSIneedle</object-name>
    <property>/autopilot/internal/vert-speed-fpm</property>
    <interpolation>
      <entry><ind> -6000 </ind><dep> -60 </dep></entry>
      <entry><ind>     0 </ind><dep>   0 </dep></entry>
      <entry><ind>  6000 </ind><dep>  60 </dep></entry>
    </interpolation>
    <center>
      <x-m> 0.000 </x-m>
      <y-m> 0.084 </y-m>
      <z-m> 0.004 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
