<!--
YASIM flight model for the Beechcraft Starship I
September 2007

Based on Avanti FDM by Guillaume CHAUVAT.

SPECS:
Height: 3.66m			(12 ft)
Length: 14m			(46 ft)
Wing span: 16.50m		(54 ft)
Max level spd: 622 km/h		(336 kts)
Max TO weight: 5800m		(19000 ft)
-->

<airplane mass="13583">

    <approach speed="90" aoa="7" >
        <control-setting axis="/controls/engines/engine[0]/throttle" value="0.2"/>
        <control-setting axis="/controls/engines/engine[0]/condition" value="1.0"/>
        <control-setting axis="/controls/engines/engine[0]/propeller-pitch" value="0.3"/>
        <control-setting axis="/controls/engines/engine[1]/throttle" value="0.2"/>
        <control-setting axis="/controls/engines/engine[1]/condition" value="1.0"/>
        <control-setting axis="/controls/engines/engine[1]/propeller-pitch" value="0.3"/>
        <control-setting axis="/controls/flight/flaps" value="1.0"/>
        <control-setting axis="/controls/gear/gear-down" value="1"/>
        <solve-weight idx="0" weight="300"/>
        <solve-weight idx="1" weight="1500"/>
    </approach>

    <cruise speed="336" alt="19000" fuel="0.8"> 
        <control-setting axis="/controls/flight/elevator-trim" value="0.0"/>
        <control-setting axis="/controls/engines/engine[0]/throttle" value="0.8"/>
        <control-setting axis="/controls/engines/engine[0]/condition" value="1.0"/>
        <control-setting axis="/controls/engines/engine[0]/propeller-pitch" value="0.9"/>
        <control-setting axis="/controls/engines/engine[1]/throttle" value="0.8"/>
        <control-setting axis="/controls/engines/engine[1]/condition" value="1.0"/>
        <control-setting axis="/controls/engines/engine[1]/propeller-pitch" value="0.9"/>
        <control-setting axis="/controls/flight/flaps" value="0.0"/>
        <control-setting axis="/controls/gear/gear-down" value="0"/>
        <solve-weight idx="0" weight="300"/>
        <solve-weight idx="1" weight="1500"/>
    </cruise>

    <cockpit x="3.389" y="0.420" z="0.476"/>

  <!-- fuselage -->
  <fuselage ax="7.004" ay="0" az="-0.486" 
            bx="-6.491" by="0" bz="-0.002"
            width="2.0" taper="0" midpoint="0.5" idrag="0.1"/>

  <!-- engines -->
  <fuselage ax="0.2"  ay="1.05" az="-0.15" 
            bx="-4.0" by="-1.05" bz="-0.15"
            width="0.9" taper="0.3" idrag="0.4"/>

  <fuselage ax="0.2"  ay="-1.05" az="-0.15" 
            bx="-4.0" by="1.05" bz="-0.15"
            width="0.9" taper="0.3" idrag="0.4"/>

  <!-- wings. Some values are unknow, so I set them to a "standard" value. -->

  <!-- Main wings -->
  <wing x="-1.7" 
        y="1.0" 
        z="-0.37" 
        length="6.0" 
        chord="1.6"
        sweep="1.0" 
        dihedral="2" 
        taper="0.375"  
        camber="0.2"
        incidence="1.0" 
        twist="-1.0" >
        <stall aoa="16" width="6" peak="1.5"/>
        <flap0 start="0.05" end=".65" lift="1.9" drag="1.5"/>
        <flap1 start=".65" end=".95" lift="1.4" drag="1.2"/>

        <control-input axis="/controls/flight/flaps" control="FLAP0"/>
        <control-input axis="/controls/flight/aileron" control="FLAP1"/>
        <control-input axis="/controls/flight/aileron" control="FLAP1" split="true"/>
        <control-input axis="/controls/flight/aileron-trim" control="FLAP1" split="true"/>

        <control-output control="FLAP0" prop="/surface-positions/flap-pos-norm"/>
        <control-output control="FLAP1" side="left" prop="/surface-positions/left-aileron-pos-norm"/>
        <control-output control="FLAP1" side="right" prop="/surface-positions/right-aileron-pos-norm"/>

        <control-speed control="FLAP0" transition-time="64.0"/>
        <control-speed control="FLAP1" transition-time="0.2"/>
  </wing>

  <!-- forward wings (canards) -->
  <mstab x="5.7" 
         y="0.5" 
         z="-1.1" 
         length="1.3" 
         chord="0.7"
         sweep="3.0" 
         dihedral="-3" 
         taper="0.8"  
         camber="0.15" 
         incidence="2.0" 
         twist="-0.0">
         <stall aoa="16" width="6" peak="1.5" />
         <flap0 start="0.1" end="0.98" lift="1.8" drag="1.6"/>

         <control-input axis="/controls/flight/flaps" control="FLAP0"/>

         <control-speed control="FLAP0" transition-time="64.0"/>

         <control-output control="FLAP0" prop="/surface-positions/flap-pos-norm"/>
  </mstab>

  <!-- hstab -->
  <hstab x="-6.2" 
         y="0.0" 
         z="1.7" 
         length="2.3" 
         chord="1.2"
         sweep="30" 
         dihedral="-3" 
         taper="0.8" 
         incidence="0" 
         camber="0.0">
         <stall aoa="11" width="4" peak="1.5"/>
         <flap0 start="0" end="1" lift="1.8" drag="1.2"/>

         <control-input axis="/controls/flight/elevator" control="FLAP0"/>
         <control-input axis="/controls/flight/elevator-trim" control="FLAP0"/>

         <control-speed control="FLAP0" transition-time="0.2"/>

         <control-output control="FLAP0" prop="/surface-positions/elevator-pos-norm"/>
  </hstab>

  <vstab x="-4.8" 
         y="0.00" 
         z="-0.2" 
         length="2.5" 
         chord="2.4"
         sweep="45" 
         taper="0.85">
         <stall aoa="16" width="4" peak="1.5"/>
         <flap0 start="0" end="1" lift="1.3" drag="1.1"/>

         <control-input axis="/controls/flight/rudder" control="FLAP0" invert="true"/>
         <control-input axis="/controls/flight/rudder-trim" control="FLAP0" invert="true"/>

         <control-speed control="FLAP0" transition-time="0.15"/>

         <control-output control="FLAP0" prop="/surface-positions/rudder-pos-norm"/>
  </vstab>

  <!-- bidules -->
  <vstab x="-4.5" y="0.3" z="-0.7" length="0.5" chord="2.5"
    sweep="80" taper="0.2" dihedral="-45">
    <stall aoa="11" width="4" peak="1.5"/>
  </vstab>

  <vstab x="-4.5" y="-0.3" z="-0.7" length="0.5" chord="2.5"
    sweep="80" taper="0.2" dihedral="225">
    <stall aoa="11" width="4" peak="1.5"/>
  </vstab>

  <!-- 2848 lbs in the wings (Jet A - 6.84 ppg ) -->
  <tank x="-1.7" y="2.5" z="-0.2" capacity="1424" jet="true"/>
  <tank x="-1.7" y="-2.5" z="-0.2" capacity="1424" jet="true"/>

  <!-- Engines: 2 Pratt & Whitney PT6A-67As - 1200 shp - Prop diam: 2.3 m -->
  <propeller x="-2.897" 
             y="1.558" 
	     z="0.097"
             mass="400" 
             moment="35" 
             radius="1.15"
             cruise-speed="380" 
             cruise-rpm="2000"
             cruise-power="1200" 
             cruise-alt="19000"
             min-rpm="150" 
             max-rpm="2000"
             fine-stop="0.25" 
             coarse-stop="3.0"
             gear-ratio="0.66">
             <turbine-engine
               eng-power="1200" 
               eng-rpm="3000" 
               alt="19000"
               flat-rating="1200"
               min-n2="70" 
               max-n2="100"
               bsfc="0.35"/>
             <actionpt x="-5.568" y="1.558" z="0.191"/>
             <control-input axis="/controls/engines/engine[0]/throttle" control="THROTTLE" src0="0.3" src1="1.0" dst0="0.0" dst1="1.0"/>
             <control-input axis="/controls/engines/engine[0]/condition" control="CONDLEVER"/>
             <control-input axis="/controls/engines/engine[0]/starter" control="STARTER"/>
             <control-input axis="/controls/engines/engine[0]/propeller-pitch" control="ADVANCE" src0="0.15" src1="1.0" dst0="-0.1" dst1="1.0"/>
             <control-input axis="/controls/engines/engine[0]/propeller-pitch" control="PROPFEATHER"  src0="0.0" src1="0.15" dst0="1.0" dst1="0.0"/>
    </propeller>

  <propeller x="-2.897" 
             y="-1.558" 
             z="0.097"
             mass="400" 
             moment="-35" 
             radius="1.15"
             cruise-speed="380" 
             cruise-rpm="2000"
             cruise-power="1200" 
             cruise-alt="19000"
             min-rpm="150" 
             max-rpm="2000"
             fine-stop="0.25" 
             coarse-stop="3.0"
             gear-ratio="0.66">
             <turbine-engine
               eng-power="1200" 
               eng-rpm="3000" 
               alt="19000"
               flat-rating="1200"
               min-n2="70" 
               max-n2="100"
               bsfc="0.35"/>
             <actionpt x="-5.568" y="-1.558" z="0.191"/>
             <control-input axis="/controls/engines/engine[1]/throttle" control="THROTTLE" src0="0.3" src1="1.0" dst0="0.0" dst1="1.0"/>
             <control-input axis="/controls/engines/engine[1]/condition" control="CONDLEVER"/>
             <control-input axis="/controls/engines/engine[1]/starter" control="STARTER"/>
             <control-input axis="/controls/engines/engine[1]/propeller-pitch" control="ADVANCE" src0="0.15" src1="1.0" dst0="-0.1" dst1="1.0"/>
             <control-input axis="/controls/engines/engine[1]/propeller-pitch" control="PROPFEATHER"  src0="0.0" src1="0.15" dst0="1.0" dst1="0.0"/>
  </propeller>


  <!-- nose -->
  <gear x="5.139" 
        y="0" 
        z="-1.800" 
        compression=".3" retract-time="5" spring="0.8" damp="1.0">
        <control-input axis="/controls/flight/rudder" control="STEER" square="true" src0="-1.0" src1="1.0" dst0="-0.8" dst1="0.8"/>
        <control-input axis="/controls/gear/gear-down" control="EXTEND"/>

        <control-speed control="EXTEND" transition-time="5"/>

        <control-output control="EXTEND" prop="/gear/gear[0]/position-norm"/>
  </gear>

  <!-- left main -->
  <gear x="-1.735" 
        y="2.556" 
        z="-1.825" 
        compression=".4" retract-time="5" spring="0.8" damp="1.2" sfric="1.0" dfric="1.0">
        <control-input axis="/controls/gear/brake-left" control="BRAKE"/>
        <control-input axis="/controls/gear/brake-parking" control="BRAKE"/>
        <control-input axis="/controls/gear/gear-down" control="EXTEND"/>

        <control-speed control="EXTEND" transition-time="5.1"/>

        <control-output control="EXTEND" prop="/gear/gear[1]/position-norm"/>
  </gear>

  <!-- right main -->
  <gear x="-1.735" 
        y="-2.556" 
        z="-1.825" 
        compression=".4" retract-time="5" spring="0.8" damp="1.2" sfric="1.0" dfric="1.0">
        <control-input axis="/controls/gear/brake-left" control="BRAKE"/>
        <control-input axis="/controls/gear/brake-parking" control="BRAKE"/>
        <control-input axis="/controls/gear/gear-down" control="EXTEND"/>

        <control-speed control="EXTEND" transition-time="5.1"/>

        <control-output control="EXTEND" prop="/gear/gear[2]/position-norm"/>
  </gear>


  <ballast x="1.4" y="-2.65" z="-0.5" mass="1200"/>
  <ballast x="1.4" y="2.65" z="-0.5" mass="1200"/>

  <!-- Cockpit mass and Avionics -->
  <ballast x="4.2" y="0.0" z="0.0" mass="500"/>

  <!-- NoseGear assembly -->
  <ballast x="5.0" y="0.0" z="-1" mass="500"/>

  <!-- MainGear assembly -->
  <ballast x="-0.9" y="0.4" z="-1.0" mass="600"/>
  <ballast x="-0.9" y="-0.4" z="-1.0" mass="600"/>

  <!-- CG -->
  <ballast x="-5.0" y="0.0" z="-0.8" mass="2000"/>

  <weight x="4.2" y="0" z="0" mass-prop="/sim/weight[0]/weight-lb"/>
  <weight x="0" y="0" z="0" mass-prop="/sim/weight[1]/weight-lb"/>

</airplane>
